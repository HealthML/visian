{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "compile": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn tsc --noEmit --pretty -p apps/api/tsconfig.app.json"
              }
            ]
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/api"]
        }
      }
    },
    "ui-shared": {
      "root": "libs/ui-shared",
      "sourceRoot": "libs/ui-shared/src",
      "projectType": "library",
      "architect": {
        "compile": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn tsc --noEmit --pretty -p libs/ui-shared/tsconfig.lib.json"
              }
            ]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui-shared/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui-shared/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/ui-shared"]
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui-shared/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/ui-shared",
            "config": {
              "configFolder": "libs/ui-shared/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-shared-e2e": {
      "root": "apps/ui-shared-e2e",
      "sourceRoot": "apps/ui-shared-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-shared-e2e/cypress.json",
            "tsConfig": "apps/ui-shared-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-shared:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui-shared:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ui-shared-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ar-demo": {
      "root": "apps/ar-demo",
      "sourceRoot": "apps/ar-demo/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ar-demo",
            "index": "apps/ar-demo/src/index.html",
            "main": "apps/ar-demo/src/main.tsx",
            "polyfills": "apps/ar-demo/src/polyfills.ts",
            "tsConfig": "apps/ar-demo/tsconfig.app.json",
            "assets": [
              "apps/ar-demo/src/favicon.ico",
              "apps/ar-demo/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "webpack.config"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ar-demo/src/environments/environment.ts",
                  "with": "apps/ar-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "compile": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn tsc --noEmit --pretty -p apps/ar-demo/tsconfig.app.json"
              }
            ]
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "ar-demo:build",
            "proxyConfig": "apps/ar-demo/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "ar-demo:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ar-demo/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ar-demo/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/ar-demo"]
        }
      }
    },
    "ar-demo-e2e": {
      "root": "apps/ar-demo-e2e",
      "sourceRoot": "apps/ar-demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ar-demo-e2e/cypress.json",
            "tsConfig": "apps/ar-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "ar-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ar-demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ar-demo-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "editor": {
      "root": "apps/editor",
      "sourceRoot": "apps/editor/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/editor",
            "index": "apps/editor/src/index.html",
            "main": "apps/editor/src/main.tsx",
            "polyfills": "apps/editor/src/polyfills.ts",
            "tsConfig": "apps/editor/tsconfig.app.json",
            "assets": ["apps/editor/src/favicon.ico", "apps/editor/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "webpack.config"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/editor/src/environments/environment.ts",
                  "with": "apps/editor/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "compile": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn tsc --noEmit --pretty -p apps/editor/tsconfig.app.json"
              }
            ]
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "editor:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "editor:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/editor/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/editor/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/editor"]
        }
      }
    },
    "editor-e2e": {
      "root": "apps/editor-e2e",
      "sourceRoot": "apps/editor-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/editor-e2e/cypress.json",
            "tsConfig": "apps/editor-e2e/tsconfig.e2e.json",
            "devServerTarget": "editor:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "editor:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/editor-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "architect": {
        "compile": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn tsc --noEmit --pretty -p libs/utils/tsconfig.lib.json"
              }
            ]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rendering": {
      "root": "libs/rendering",
      "sourceRoot": "libs/rendering/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/rendering/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/rendering"],
          "options": {
            "jestConfig": "libs/rendering/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "editor",
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  }
}
